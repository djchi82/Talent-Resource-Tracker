<h1>Listing skill_sets</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @skill_sets.each do |skill_set| %>
  <tr>
    <td><%= link_to skill_set.name, skill_set %></td>
    <td><%= skill_set.description %></td>
    <td><%= link_to 'Show', skill_set %></td>
    <td><%= link_to 'Edit', edit_skill_set_path(skill_set) %></td>
    <td><%= link_to 'Destroy', skill_set, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Skill set', new_skill_set_path %>
